{
  "name": "oof-bar/craft-do-apps",
  "description": "Craft CMS for Digital Ocean Apps Platform",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "digitalocean",
    "infrastructure",
    "paas"
  ],
  "license": "0BSD",
  "homepage": "https://github.com/oof-bar/craft-do-apps/",
  "type": "starterkit",
  "require": {
    "ext-bcmath": "*",
    "ext-imagick": "*",
    "ext-intl": "*",
    "ext-mbstring": "*",
    "craftcms/cms": "^4.0.0",
    "vaersaagod/dospaces": "^2.0.0",
    "vlucas/phpdotenv": "^5.4.0",
    "yiisoft/yii2-redis": "^2.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
